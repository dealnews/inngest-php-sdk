{
    "name": "dealnews/inngest-php-sdk",
    "description": "Unofficial PHP SDK for Inngest - Build event-driven workflows with durable execution",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": ["inngest", "event-driven", "workflow", "serverless", "queue", "background-jobs"],
    "authors": [
        {
            "name": "Brian Moon",
            "email": "brianm@dealnews.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-hash": "*",
        "psr/http-message": "^1.0|^2.0",
        "psr/http-client": "^1.0",
        "psr/log": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "DealNews\\Inngest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DealNews\\Inngest\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
